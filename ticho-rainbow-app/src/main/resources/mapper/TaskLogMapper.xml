<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.ticho.rainbow.infrastructure.mapper.TaskLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="TaskLog_map" type="top.ticho.rainbow.infrastructure.entity.TaskLog">
        <result column="id" property="id"/>
        <result column="task_id" property="taskId"/>
        <result column="content" property="content"/>
        <result column="param" property="param"/>
        <result column="cron_expression" property="cronExpression"/>
        <result column="execute_time" property="executeTime"/>
        <result column="start_time" property="startTime"/>
        <result column="end_time" property="endTime"/>
        <result column="consume" property="consume"/>
        <result column="mdc" property="mdc"/>
        <result column="trace_id" property="traceId"/>
        <result column="status" property="status"/>
        <result column="operate_by" property="operateBy"/>
        <result column="create_time" property="createTime"/>
        <result column="is_err" property="isErr"/>
        <result column="err_message" property="errMessage"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="TaskLog_sql">
        id, task_id, content, param, cron_expression, execute_time, start_time, end_time, consume, mdc, trace_id, `status`, operate_by, create_time, is_err, err_message
    </sql>


</mapper>
